{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@gae-web/appengine-api": [
        "dist/gae-web/appengine-api"
      ],
      "@gae-web/appengine-api/*": [
        "dist/gae-web/appengine-api/*"
      ],
      "@gae-web/appengine-client": [
        "dist/gae-web/appengine-client"
      ],
      "@gae-web/appengine-client/*": [
        "dist/gae-web/appengine-client/*"
      ],
      "@gae-web/appengine-utility": [
        "dist/gae-web/appengine-utility"
      ],
      "@gae-web/appengine-utility/*": [
        "dist/gae-web/appengine-utility/*"
      ],
      "@gae-web/appengine-token": [
        "dist/gae-web/appengine-token"
      ],
      "@gae-web/appengine-token/*": [
        "dist/gae-web/appengine-token/*"
      ]
    }
  }
}