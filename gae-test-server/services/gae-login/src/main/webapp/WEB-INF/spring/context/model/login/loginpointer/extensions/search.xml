<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:security="http://www.springframework.org/schema/security"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc.xsd http://www.springframework.org/schema/security http://www.springframework.org/schema/security/spring-security.xsd http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	<!--Query Services -->
	<bean
		class="com.dereekb.gae.model.extension.search.query.service.impl.ModelQueryServiceImpl"
		id="loginPointerQueryService">
		<constructor-arg index="0" ref="loginPointerRegistry" />
		<constructor-arg index="1"
			ref="securedLoginPointerQueryInitializer" />
	</bean>
	<bean
		class="com.dereekb.gae.server.datastore.objectify.query.impl.TaskedObjectifyQueryRequestLimitedBuilderInitializer"
		id="securedLoginPointerQueryInitializer">
		<constructor-arg index="0" ref="loginPointerQueryInitializer" />
		<constructor-arg index="1">
			<bean
				class="com.dereekb.gae.server.auth.security.model.query.task.impl.LoginSecurityModelQueryTaskOverrideImpl">
				<constructor-arg index="0">
					<list>
						<ref bean="loginOwnedModelQuerySecurityDelegate" />
					</list>
				</constructor-arg>
			</bean>
		</constructor-arg>
	</bean>
	<bean
		class="com.dereekb.gae.server.auth.model.pointer.search.query.LoginPointerQueryInitializer"
		id="loginPointerQueryInitializer" />
	<!--Security -->
	<!--Owned Model -->
	<bean
		class="com.dereekb.gae.server.auth.model.pointer.misc.owned.query.security.LoginPointerOwnedModelQuerySecurityDelegate"
		id="loginPointerOwnedModelQuerySecurityDelegate">
		<constructor-arg index="0"
			ref="anonymousModelRoleSetContextService" />
	</bean>
	<bean
		class="com.dereekb.gae.server.auth.model.pointer.misc.owned.query.security.LoginPointerOwnedModelQuerySecurityDelegate"
		id="optionalLoginPointerOwnedModelQuerySecurityDelegate">
		<constructor-arg index="0"
			ref="anonymousModelRoleSetContextService" />
		<property name="optional" value="true" />
	</bean>
</beans>
